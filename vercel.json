{
  "rewrites": [
    {
      "source": "/:path*/",
      "destination": "/:path*.html"
    },
    {
      "source": "/index",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    { "source": "/calc.html", "destination": "/calc", "permanent": true },
    { "source": "/index.html", "destination": "/", "permanent": true },
    { "source": "/:path*.html", "destination": "/:path*", "permanent": true }
  ]
}
